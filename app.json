{
  "expo": {
    "name": "SwatchBoard",
    "slug": "collage-maker",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "description": "Create beautiful photo collages with ease. Choose from multiple templates and customize your layouts.",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jordanryan.collagemaker",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to create collages.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save collages to your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.jordanryan.collagemaker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "1738a900-6e48-452a-9541-0ee4b98dd3da"
      }
    },
    "owner": "jordanrtesting"
  }
}
